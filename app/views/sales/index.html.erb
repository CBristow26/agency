          <div class="hero-buttons-wrapper">

          <%= link_to "About Us", sales_about_path, class: "hero-button", :"data-ix" =>"hero-button" %>
          
          <%= link_to "Contact Us", sales_contact_path, class: "_2 hero-button", :"data-ix" =>"hero-button-2" %>

          </div>
        </div><img class="hero-image macbook" sizes="(max-width: 479px) 90vw, (max-width: 767px) 93vw, (max-width: 991px) 88vw, 834.375px" src="assets/Macbook.png" srcset="assets/Product-slider-1-p-500x264.png 500w, assets/Product-slider-1-p-800x422.png 800w, assets/Product-slider-1-p-1080x570.png 1080w, assets/Macbook.png 1500w"><img class="hero-image phone-white" src="assets/Mockup-iphone2.png"><img class="hero-image" src="assets/Mockup-iphone.png">
